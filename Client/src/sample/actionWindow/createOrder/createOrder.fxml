<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.actionWindow.createOrder.CreateOrder">
   <children>
      <VBox prefHeight="400.0" prefWidth="166.0">
         <children>
             <Label alignment="CENTER" contentDisplay="CENTER" text="Меню пользователя" />
             <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Введите ваш адрес" />
             <TextField fx:id="info" prefHeight="25.0" prefWidth="100.0" />
             <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Введите ваш email" />
             <TextField fx:id="email" prefHeight="25.0" prefWidth="100.0" />
             <Button mnemonicParsing="false" onAction="#sendOrder" text="Оформление заказа" />
         </children>
      </VBox>
   </children>
</AnchorPane>
